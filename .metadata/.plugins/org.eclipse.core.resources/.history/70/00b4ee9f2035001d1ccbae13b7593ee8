package code_forces;
import java.io.*;

import java.util.*;

class A {
   
    public static void main(String args[] ) throws Exception {
     Scanner sc=new Scanner(System.in);
    
     int n=sc.nextInt();
     int a[]= {2,4,3,7,0};
      bubleSort(a,a.length);
     
    }
   static void bubbleSort(int a[],int n) {
    	for(int i=0;i<n;i++) {
    		for(int j=i+1;j<n;j++) {
    			if(a[i]>a[j]) {
    				int temp=a[i];
    				a[i]=a[j];
    				a[j]=temp;
    			}
    		}
    	}
    	System.out.println(Arrays.toString(a));
    }
}

